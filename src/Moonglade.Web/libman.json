{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "tinymce@5.4.1",
      "destination": "wwwroot/lib/tinymce",
      "files": []
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "library": "bootstrap-tagsinput@0.8.0",
      "destination": "wwwroot/lib/bootstrap-tagsinput/",
      "files": [
        "bootstrap-tagsinput.min.js",
        "bootstrap-tagsinput-typeahead.css",
        "bootstrap-tagsinput.css"
      ]
    },
    {
      "library": "simplemde@1.11.2",
      "destination": "wwwroot/lib/simplemde/"
    },
    {
      "library": "monaco-editor@0.20.0",
      "destination": "wwwroot/lib/monaco-editor/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.5.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bs-custom-file-input@1.3.4",
      "destination": "wwwroot/lib/bs-custom-file-input/",
      "files": [
        "dist/bs-custom-file-input.min.js",
        "dist/bs-custom-file-input.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    }
  ]
}